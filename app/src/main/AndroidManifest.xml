<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.xiaohong.bilibilivideo">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_bilibili"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".activitys.VideoDetailActivity">
        </activity>
        <activity android:name=".activitys.MenuActivity">
        </activity>
        <activity android:name=".activitys.WebViewActivity">
        </activity>
        <activity android:name=".activitys.LiveVideoActivity">
        </activity>
    </application>

</manifest>